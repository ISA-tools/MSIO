property_value: IAO:0000117 "PERSON: Bjoern Peters" xsd:string
property_value: IAO:0000117 "PERSON: Philippe Rocca-Serra" xsd:string
property_value: IAO:0000117 "PERSON: Susanna Sansone" xsd:string
