    <!-- http://purl.obolibrary.org/obo/MSIO_0000185 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MSIO_0000185">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MSIO_0000096"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/OBI_0000293"/>
